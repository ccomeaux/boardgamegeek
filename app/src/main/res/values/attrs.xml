<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="StatBar">
		<attr name="drawable" format="reference"/>
	</declare-styleable>
	<declare-styleable name="GameDetailRow">
		<attr name="label" format="string"/>
		<attr name="icon_res" format="reference"/>
		<attr name="query_token" format="integer"/>
	</declare-styleable>
	<declare-styleable name="BezelImageView">
		<attr name="maskDrawable" format="reference"/>
		<attr name="borderDrawable" format="reference"/>
		<attr name="desaturateOnPress" format="boolean"/>
	</declare-styleable>
	<declare-styleable name="BggForegroundLinearLayout">
		<attr name="android:foreground"/>
		<attr name="foregroundInsidePadding"/>
		<attr name="android:foregroundGravity"/>
	</declare-styleable>
	<declare-styleable name="ForegroundImageView">
		<attr name="android:foreground"/>
		<attr name="foregroundInsidePadding"/>
		<attr name="android:foregroundGravity"/>
	</declare-styleable>
	<declare-styleable name="TimestampView">
		<attr name="isForumTimestamp" format="boolean"/>
		<attr name="includeTime" format="boolean"/>
		<attr name="format" format="string"/>
		<attr name="emptyMessage" format="string"/>
		<attr name="hideWhenEmpty" format="boolean"/>
	</declare-styleable>
	<declare-styleable name="TextEditorView">
		<attr name="headerText" format="string"/>
	</declare-styleable>
	<declare-styleable name="RatingView">
		<attr name="hideWhenZero" format="boolean"/>
	</declare-styleable>
	<declare-styleable name="DataStepRow">
		<attr name="titleLabel" format="string"/>
		<attr name="descriptionLabel" format="string"/>
	</declare-styleable>
</resources>
